    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-es2016": "^6.24.1",
    "eslint": "^5.3.0",
    "ghooks": "^2.0.4",
    "jest": "^23.4.2",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "ajv": "^6.5.2"
  },
  "jest": {
    "testURL": "http://localhost/"
